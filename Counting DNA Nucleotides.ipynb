    "dnastr = input()\n",
    "l = len(dnastr)\n",
    "A = 0;C = 0;G = 0;T = 0\n",
    "for i in range(0,l):\n",
    "    if dnastr[i] == 'A':\n",
    "        A += 1\n",
    "    if dnastr[i] == 'C':\n",
    "        C += 1\n",
    "    if dnastr[i] == 'G':\n",
    "        G += 1\n",
    "    if dnastr[i] == 'T':\n",
    "        T += 1\n",
    "print (A,\" \",C,\" \",G,\" \",T)"
   ]
